<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
            http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/aop
            http://www.springframework.org/schema/aop/spring-aop.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-2.5.xsd  
            ">
	<bean id="shineClusterChannelName" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="PAMIRS_SHINE" />
	</bean>

	<bean id="shineClusterChannel" class="com.taobao.pamirs.cache.store.ExtendedChannel"
		factory-method="getInstance">
	</bean>


	<bean id="methodSyncServerBean" class="com.taobao.pamirs.cache.event.MethodSyncServerBean">
		<property name="channel" ref="shineClusterChannel" />
	</bean>
	
	   <bean class="com.taobao.pamirs.cache.proxy.CacheAspect"
        factory-method="aspectOf">
        <property name="methodSyncServerBean" ref="methodSyncServerBean" />
    </bean>
    
    <bean id="contextApplicationContextProvider" class="com.taobao.pamirs.cache.store.ApplicationContextProvider"></bean>
    

	<aop:aspectj-autoproxy proxy-target-class="true" />
</beans>
